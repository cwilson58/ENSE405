<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EatingHabitAnalyzerApp.FeelingsQuiz"
             Title="Feelings Quiz">
    <VerticalStackLayout>
        <Label Text="Welcome To Your Daily Feelings Quiz!"/>
        <Label Text="How do you feel today?"/>
        <RadioButton Content="Very Poor"/>
        <RadioButton Content="Poor"/>
        <RadioButton Content="Neutral" IsChecked="True"/>
        <RadioButton Content="Good"/>
        <RadioButton Content="Very Good"/>
    </VerticalStackLayout>
</ContentPage>